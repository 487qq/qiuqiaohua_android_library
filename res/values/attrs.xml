<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="DashedLineView">
        <attr name="orientation">
            <enum name="horizontal" value="0"></enum>
            <enum name="vertical" value="1"></enum>
        </attr>
        <attr name="lineType">
            <enum name="dash" value="0"></enum>
            <enum name="real" value="1"></enum>
        </attr>
        <attr name="lineColor" format="color" />
        
    </declare-styleable>

</resources>